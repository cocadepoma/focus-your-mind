    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.208",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  }
}
